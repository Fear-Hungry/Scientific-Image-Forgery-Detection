{
  "name": "fft_cls_logmag_256",
  "fft": {
    "mode": "logmag",
    "input_size": 256,
    "hp_radius_fraction": 0.1,
    "normalize_percentiles": [5.0, 95.0]
  },
  "model": {
    "backbone": "resnet18",
    "dropout": 0.0
  },
  "train": {
    "epochs": 5,
    "batch_size": 32,
    "lr": 0.001,
    "weight_decay": 0.0001,
    "num_workers": 2,
    "val_fraction": 0.1,
    "seed": 42,
    "folds": 1,
    "fold": -1,
    "no_aug": false,
    "scheduler": "none",
    "lr_min": 1e-6,
    "max_lr": 0.0,
    "pct_start": 0.1
  }
}
